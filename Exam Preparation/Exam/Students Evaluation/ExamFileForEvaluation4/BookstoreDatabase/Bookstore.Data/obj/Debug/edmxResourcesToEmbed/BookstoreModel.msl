    <AssociationSetMapping Name="Books_Authors" TypeName="BookstoreDBModel.Books_Authors" StoreEntitySet="Books_Authors">
      <EndProperty Name="Authors">
        <ScalarProperty Name="AuthorId" ColumnName="AuthorId" />
      </EndProperty>
