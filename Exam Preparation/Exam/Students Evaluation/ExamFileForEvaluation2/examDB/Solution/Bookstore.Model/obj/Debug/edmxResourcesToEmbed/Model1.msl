        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <AssociationSetMapping Name="BooksAutors" TypeName="BookstoreModel.BooksAutors" StoreEntitySet="BooksAutors">
      <EndProperty Name="Authors">
        <ScalarProperty Name="ID" ColumnName="AuthorID" />
